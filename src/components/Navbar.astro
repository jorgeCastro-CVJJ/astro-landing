---
import pages from '../config/pages.json'
---

<nav
    class="big-zinc-600 flex flex-col md:flex-row p-5 gap-5 md:text-left rounded-xl"
>
    <div class="bg-black min-w-full p-5 ">
        <ul class="text-2xl font-semibold flex float-left gap-8">
       {
        pages.map((page) => (
            <il><a href={page.route}>{page.title}</a></il>
        ))
       }
       </ul>
       
        <img src="../../public/favicon.svg" class="flex float-right w-8">
        
    </div>
</nav>
